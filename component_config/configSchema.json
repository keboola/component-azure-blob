{
  "type": "object",
  "title": "Parameters",
  "required": [
      "account_name",
      "#account_key",
      "container_name",
      "destination_path",
      "append_date_to_file"
  ],
  "properties": {
    "account_name": {
      "description": "Azure Storage Resources Account Name",
      "type": "string",
      "title": "Account Name",
      "minLength": 1,
      "propertyOrder": 1
    },
    "#account_key": {
      "type": "string",
      "description": "Azure Storage Resources Account Key",
      "title": "Account Key",
      "format": "password",
      "default": "",
      "propertyOrder": 2
    },
    "container_name": {
      "type": "string",
      "title": "Container Name",
      "description": "Azure Storage Container Name",
      "minLength": 1,
      "propertyOrder": 4
    },
    "destination_path": {
      "type": "string",
      "title": "Destination Path",
      "description": "The folder path you want to export the files to. Please refer to component description on how to configure",
      "propertyOrder": 5
    },
    "append_date_to_file": {
      "type": "boolean",
      "title": "Append Date to File",
      "format": "checkbox",
      "description": "Today's date (YYYY_MM_dd) will be appended into the export file",
      "propertyOrder": 6,
      "default": false
    }
  }
}